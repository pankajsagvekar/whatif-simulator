{
  "name": "whatif-simulator",
  "version": "1.0.0",
  "description": "AI What If Simulator - Generate serious and fun outcomes for hypothetical scenarios",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:react": "vite build",
    "dev": "ts-node src/index.ts",
    "dev:react": "vite",
    "preview:react": "vite preview",
    "demo": "ts-node src/demo-simulator.ts",
    "cli": "ts-node src/cli-app.ts",
    "cli:interactive": "ts-node src/cli-app.ts interactive",
    "cli:demo": "ts-node src/cli-app.ts demo",
    "cli:test": "ts-node src/cli-app.ts test",
    "cli:health": "ts-node src/cli-app.ts health",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:react": "vitest --config vitest.react.config.ts",
    "test:react:run": "vitest --run --config vitest.react.config.ts",
    "test:e2e": "vitest --run src/__tests__/e2e.test.ts",
    "test:content": "vitest --run src/__tests__/content-appropriateness.test.ts",
    "test:load": "vitest --run src/__tests__/load-testing.test.ts",
    "test:integration": "vitest --run src/__tests__/comprehensive-integration.test.ts",
    "test:final": "vitest --run src/__tests__/integration-final.test.ts",
    "test:comprehensive": "vitest --run src/__tests__/",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/index.js",
    "start:cli": "node dist/cli-app.js",
    "start:demo": "node dist/demo-simulator.js"
  },
  "keywords": [
    "ai",
    "simulator",
    "whatif",
    "scenarios"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^5.0.1",
    "eslint": "^8.0.0",
    "jsdom": "^26.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.1.3",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@types/styled-components": "^5.1.34",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reactbits-animation": "^1.0.0",
    "styled-components": "^6.1.19"
  }
}
