{
  "name": "whatif-simulator",
  "version": "1.0.0",
  "description": "AI What If Simulator - Generate serious and fun outcomes for hypothetical scenarios",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "demo": "ts-node src/demo-simulator.ts",
    "cli": "ts-node src/cli-app.ts",
    "cli:interactive": "ts-node src/cli-app.ts interactive",
    "cli:demo": "ts-node src/cli-app.ts demo",
    "cli:test": "ts-node src/cli-app.ts test",
    "cli:health": "ts-node src/cli-app.ts health",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:e2e": "vitest --run src/__tests__/e2e.test.ts",
    "test:content": "vitest --run src/__tests__/content-appropriateness.test.ts",
    "test:load": "vitest --run src/__tests__/load-testing.test.ts",
    "test:integration": "vitest --run src/__tests__/comprehensive-integration.test.ts",
    "test:final": "vitest --run src/__tests__/integration-final.test.ts",
    "test:comprehensive": "vitest --run src/__tests__/",
    "test:coverage": "vitest --run --coverage",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "start": "node dist/index.js",
    "start:cli": "node dist/cli-app.js",
    "start:demo": "node dist/demo-simulator.js"
  },
  "keywords": ["ai", "simulator", "whatif", "scenarios"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {}
}